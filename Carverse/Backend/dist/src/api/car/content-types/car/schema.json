{
    "kind": "collectionType",
    "collectionName": "cars",
    "info": {
        "singularName": "car",
        "pluralName": "cars",
        "displayName": "cars"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string"
        },
        "price": {
            "type": "integer"
        },
        "image": {
            "type": "media",
            "multiple": true,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "description": {
            "type": "richtext"
        },
        "modelyear": {
            "type": "integer"
        },
        "category": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::category.category",
            "inversedBy": "cars"
        }
    }
}
